    count1 = value + 1
    while count1 > 0:
        sq_sum = []
        for i in range(count1):
            sq_sum.append(i)
            count1 = count1 - 1
        sq_sum = sum(sq_sum)**2
    return sq_sum - sum_sq


>>> sum_sq(10)
2640
>>> sum_sq(100)
25502115
